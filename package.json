    "@angular/router": "^18.2.12",
    "@types/pg": "^8.11.10",
    "angular": "^1.6.10",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5"
  },
